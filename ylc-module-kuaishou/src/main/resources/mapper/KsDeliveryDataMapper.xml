    <delete id="deleteKsDeliveryDataById" parameterType="Long">
        delete from ks_delivery_data where id = #{id}
    </delete>

    <delete id="deleteKsDeliveryDataByIds" parameterType="String">
        delete from ks_delivery_data where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>