<mapper namespace="com.ylc.component.kuaishou.mapper.KsDeliveryDataMapper">
    
    <resultMap type="com.ylc.component.kuaishou.domain.KsDeliveryData" id="KsDeliveryDataResult">
        <result property="id"    column="id"    />
        <result property="deliveryRecordId"    column="delivery_record_id"    />
        <result property="deliveryName"    column="delivery_name"    />
        <result property="deliveryNumber"    column="delivery_number"    />
        <result property="deliveryTime"    column="delivery_time"    />
        <result property="status"    column="status"    />
        <result property="shopName"    column="shop_name"    />
