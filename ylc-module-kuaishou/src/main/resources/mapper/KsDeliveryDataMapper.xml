    <update id="updateKsDeliveryData" parameterType="com.ylc.component.kuaishou.domain.KsDeliveryData">
        update ks_delivery_data
        <trim prefix="SET" suffixOverrides=",">
            <if test="deliveryRecordId != null">delivery_record_id = #{deliveryRecordId},</if>
            <if test="deliveryName != null">delivery_name = #{deliveryName},</if>
            <if test="deliveryNumber != null">delivery_number = #{deliveryNumber},</if>
            <if test="deliveryTime != null">delivery_time = #{deliveryTime},</if>
            <if test="status != null">status = #{status},</if>
            <if test="shopName != null">shop_name = #{shopName},</if>
            <if test="nativeNumber != null">native_number = #{nativeNumber},</if>
            <if test="serialNumber != null">serial_number = #{serialNumber},</if>
            <if test="deliveryDescribe != null">delivery_describe = #{deliveryDescribe},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
