        <result property="signTime" column="sign_time"/>
        <result property="signStatus" column="sign_status"/>
    </resultMap>

    <sql id="selectKsDeliveryDataVo"> select id, delivery_record_id, delivery_name, delivery_number, delivery_time, status, shop_name, native_number, serial_number, delivery_describe, del_flag, create_by, create_time, update_by, update_time, remark, sign_time, sign_status from ks_delivery_data </sql>

    <select id="selectKsDeliveryDataList" parameterType="com.ylc.component.kuaishou.domain.KsDeliveryData" resultMap="KsDeliveryDataResult">
        <include refid="selectKsDeliveryDataVo"/>
        <where>  
            <if test="deliveryRecordId != null "> and delivery_record_id = #{deliveryRecordId}</if>
            <if test="deliveryName != null  and deliveryName != ''"> and delivery_name like concat('%', #{deliveryName}, '%')</if>
            <if test="deliveryNumber != null  and deliveryNumber != ''"> and delivery_number = #{deliveryNumber}</if>
            <if test="deliveryTime != null "> and delivery_time = #{deliveryTime}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
            <if test="shopName != null  and shopName != ''"> and shop_name like concat('%', #{shopName}, '%')</if>
            <if test="nativeNumber != null  and nativeNumber != ''"> and native_number = #{nativeNumber}</if>
            <if test="serialNumber != null  and serialNumber != ''"> and serial_number = #{serialNumber}</if>
            <if test="deliveryDescribe != null  and deliveryDescribe != ''"> and delivery_describe = #{deliveryDescribe}</if>
            <if test="signTime != null "> and sign_time = #{signTime}</if>
            <if test="signStatus != null and signStatus != ''"> and sign_status = #{signStatus}</if>
        </where>
    </select>
    
    <select id="selectKsDeliveryDataById" parameterType="Long" resultMap="KsDeliveryDataResult">
        <include refid="selectKsDeliveryDataVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertKsDeliveryData" parameterType="com.ylc.component.kuaishou.domain.KsDeliveryData" useGeneratedKeys="true" keyProperty="id">
        insert into ks_delivery_data
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="deliveryRecordId != null">delivery_record_id,</if>
            <if test="deliveryName != null">delivery_name,</if>
            <if test="deliveryNumber != null">delivery_number,</if>
            <if test="deliveryTime != null">delivery_time,</if>
